  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MrLyfing/heartflow-theme/issues"
  },
  "homepage": "https://github.com/MrLyfing/heartflow-theme#readme",
  "dependencies": {
    "element-theme": "github:syn-zeta/element-theme",
    "element-ui": "2.3.6",
    "vue": "^2.5.16",
    "vue-resource": "^1.5.0",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-component": "^1.1.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
