{
  "name": "hf-element-ui",
  "version": "1.1.0",
  "private": true,
  "description": "Heartflow Element UI",
  "homepage": "https://github.com/HeartFlow/hf-element-ui",
  "bugs": {
    "url": "https://github.com/HeartFlow/hf-element-ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HeartFlow/hf-element-ui.git"
  },
  "author": "HeartFlow FrontEnd Team <frontend@heartflow.com>",
  "main": "bin/index.js",
  "scripts": {
    "build": "npm run clean && et && npm run build:kit",
    "build:example": "webpack --config webpack/example.js --mode production",
    "build:kit": "webpack --config webpack/kit.js --mode production",
    "analyze:kit": "webpack --config webpack/kit.js --mode production --profile --json > stats.json && webpack-bundle-analyzer stats.json",
    "clean": "rm -rf lib bin",
    "predeploy": "rm-rf example/dist && npm run build:example",
    "deploy": "gh-pages -d example/dist",
    "dev": "concurrently \"npm run serve:example\" \"npm run watch:kit\" \"et --watch\"",
    "postinstall": "npm run build",
    "serve:example": "webpack-dev-server --config webpack/example.js --mode development",
    "watch:kit": "webpack --config webpack/kit.js --mode development --watch"
  },
  "dependencies": {
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.2.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-component": "^1.1.0",
    "css-loader": "^3.2.1",
    "element-theme": "github:HeartFlow/element-theme",
    "element-ui": "2.3.6",
    "file-loader": "^5.0.2",
    "lodash.debounce": "^4.0.8",
    "mini-css-extract-plugin": "^0.8.0",
    "style-loader": "^1.0.1",
    "vue": "^2.6.10",
    "vue-click-outside": "github:vue-bulma/click-outside",
    "vue-loader": "^15.7.2",
    "vue-router": "^3.0.6",
    "vue-template-compiler": "^2.6.10",
    "vue-visible": "^1.0.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2"
  },
  "devDependencies": {
    "concurrently": "^4.1.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "gh-pages": "^2.0.1",
    "webpack-bundle-analyzer": "^3.6.0"
  },
  "element-theme": {
    "config": "./element-variables.css",
    "theme": "../theme",
    "out": "./lib",
    "watchGlobs": [
      "./element-variables.css",
      "./theme/**/*.scss"
    ],
    "components": [
      "autocomplete",
      "breadcrumb",
      "breadcrumb-item",
      "button",
      "checkbox",
      "checkbox-group",
      "col",
      "collapse",
      "collapse-item",
      "date-picker",
      "dialog",
      "dropdown",
      "dropdown-item",
      "dropdown-menu",
      "form",
      "form-item",
      "input",
      "input-number",
      "message",
      "notification",
      "option",
      "option-group",
      "popover",
      "radio",
      "radio-button",
      "radio-group",
      "row",
      "select",
      "spinner",
      "switch",
      "table",
      "table-column",
      "tooltip",
      "transfer"
    ]
  }
}
