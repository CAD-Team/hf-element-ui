{
  "name": "heartflow-theme",
  "version": "1.0.0",
  "description": "Heartflow Element-UI theme",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf lib",
    "build": "npm run clean && et && webpack",
    "kit": "webpack-dev-server",
    "watch": "et --watch",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MrLyfing/heartflow-theme.git"
